                    },
                    "mythrilVersion": {
                        "type": "string"
                    },
                    "queueTime": {
                        "type": "integer"
                    },
                    "runTime": {
                        "type": "integer"
                    },
                    "status": {
                        "type": "string"
                    },
                    "submittedAt": {
